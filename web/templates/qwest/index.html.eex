<h2>Qwests</h2>

<%= link "New qwest", to: qwest_path(@conn, :new) %>

<ul class="nav nav-tabs">
  <li class="active"><a data-toggle="tab" href="#uncompleted">Uncompleted</a></li>
  <li><a data-toggle="tab" href="#completed">Completed</a></li>
</ul>

<div class="tab-content">
  <div id="uncompleted" class="tab-pane fade in active">
    <%= render Qwestr.QwestView, "list.html", conn: @conn, qwests: @uncompleted_qwests %>
  </div>
  <div id="completed" class="tab-pane fade">
    <%= render Qwestr.QwestView, "list.html", conn: @conn, qwests: @completed_qwests %>
  </div>
</div>
